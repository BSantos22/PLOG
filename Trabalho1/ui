%Heavy lines
lt_corner(X) :- put_code(9487).
rt_corner(X) :- put_code(9491).
lb_corner(X) :- put_code(9495).
rb_corner(X) :- put_code(9499).
horiz(X) :- put_code(9473).
vert(X) :- put_code(9475).
top_con(X) :- put_code(9523).
bottom_con(X) :- put_code(9531).
left_con(X) :- put_code(9507).
right_con(X) :- put_code(9515).
middle(X) :- put_code(9547).
*/

black_circle(X) :- put_code(11044).%128308).
white_circle(X) :- put_code(9711).%11093).















